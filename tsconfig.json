        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [ "es2015", "es5", "dom"]
    },
    "include": [
        "./src/**/*"
    ]
}
